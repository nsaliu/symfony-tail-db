{
    "name": "nsaliu/symfony-tail-db",
    "description": "Symfony Tail DB Bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "doctrine/orm": "^2.8",
        "react/event-loop": "^1.1",
        "react/socket": "^1.6",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/serializer-pack": "^1.0",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "jangregor/phpstan-prophecy": "^0.8.1",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "NSaliu\\TailDb\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nsaliu\\TailDb\\Tests\\": "tests/"
        }
    }
}
